{
   "arrowParens": "always",
   "bracketSameLine": true,
   "bracketSpacing": false,
   "embeddedLanguageFormatting": "auto",
   "htmlWhitespaceSensitivity": "css",
   "insertPragma": false,
   "jsxSingleQuote": false,
   "proseWrap": "preserve",
   "quoteProps": "as-needed",
   "requirePragma": false,
   "semi": false,
   "singleAttributePerLine": true,
   "singleQuote": true,
   "tabWidth": 3,
   "trailingComma": "es5",
   "useTabs": false,
   "vueIndentScriptAndStyle": false,
   "printWidth": 80,
   "endOfLine": "lf"
}
